#include <iostream>
using namespace std;
int main()
{
  int a,b,c,d;
  cout<<"Input 2 integers a and b: "<<endl;
  cin>>a;
  cin>>b;
  cout<<(a>b)<<endl;  
  cout<<"Input 4 integers a,b,c and d: "<<endl;
  cin>>a;
  cin>>b;
  cin>>c;
  cin>>d;
  cout<<(a>b && c<d)<<endl;   //and statement
  cout<<(a>b || c<d)<<endl;   //or statement
  return 0;
}
